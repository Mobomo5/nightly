{% extends "@html5/base.twig" %}
{% block head %}
    <link rel="stylesheet" type="text/css" href="{{ site.getBaseDirectory }}includes/themes/default/css/main.css"/>
    <link rel="icon" href="{{ site.getBaseDirectory }}includes/images/educaskFavicon.ico">
{% endblock %}
{% block body %}
    <header></header>
    <div id="headerGap"></div>
    <div id="contentWrapper">
        <section id="content">
            {% for block in blocks.main %}
                <h1>{{ block.getTitle }}</h1>
                {{ block.getContent() | raw }}
            {% endfor %}
        </section>
    </div>
    <div id="sidebar">
    </div>
{% endblock %}